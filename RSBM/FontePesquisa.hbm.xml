<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RSBM"
                   namespace="RSBM.Models">
  <class name="RSBM.Models.FontePesquisa" table="fonte_pesquisa">

    <id name="Id" column="idfonte_pesquisa" >
      <generator class="increment"/>
    </id>

    <property column ="nome" name="Nome"/>

    <property  name="IdOrgao">
      <column name="idorgao" default="null"/>
    </property>

    <property  name="Portal">
      <column name="portal" default="null"/>
    </property>

    <property column ="link" name="Link" />

    <property column ="regex" name="Regex" />
    
    <property  name="UsuarioRota">
      <column name="usuario_rota" default="null"/>
    </property>

    <property column ="conf_rota" name="ConfRota" />

    <property column ="ultimo_conteudo" name="UltimoConteudo" />
    
    <property  name="AtivarRobot">
      <column name="ativar_robot" default="null"/>
    </property>

    <property column ="uf" name="Uf" />
    
    <property  name="IdCidade">
      <column name="idcidade" default="null"/>
    </property>

    <property column ="email" name="Email" />
    
    <property  name="Excluido">
      <column name="excluido" default="null"/>
    </property>

  </class>
</hibernate-mapping>

