<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RSBM"
                   namespace="RSBM.Models">
  <class name="RSBM.Models.LicitacaoHistorico" table="licitacao_historico">

    <id name="Id" column="idlicitacao_historico" >
      <generator class="native"/>
    </id>

    <property column ="idlicitacao" name="IdLicitacao"/>

    <property column ="historico" name="Historico"/>

    <property column ="data_cadastro" name="DataCadastro"/>

    <property column ="mensagem" name="Mensagem"/>

    <property column ="resposta" name="Resposta"/>

  </class>
</hibernate-mapping>

